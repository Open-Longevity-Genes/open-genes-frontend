{
  "name": "frontend",
  "version": "0.5.0",
  "scripts": {
    "static-prefixer": "postcss src/assets/sass/style.css --use=autoprefixer --map=false --output=src/assets/sass/style.css",
    "static-sass": "node-sass --output-style compressed --include-path src/assets/sass src/assets/sass/style.scss src/assets/sass/style.css",
    "static": "npm run static-sass && npm run static-prefixer",
    "static-w": "nodemon -e scss -x npm run static",
    "clear-sprites": "rm -r src/assets/images/icons/sprites",
    "spriteset-general": "svg-sprite --symbol --dest=src/assets/images/icons/sprites src/assets/images/icons/source/general/*.svg --symbol-sprite=general.sprite.svg",
    "spriteset-landing": "svg-sprite --symbol --dest=src/assets/images/icons/sprites src/assets/images/icons/source/landing/*.svg --symbol-sprite=landing.sprite.svg",
    "spritesheets": "npm run spriteset-general & npm run spriteset-landing",
    "sprites": "npm run clear-sprites && npm run spritesheets",
    "ng": "ng",
    "start": "npm run static && ng serve",
    "build": "npm run static && sprites && ng build",
    "build-staging": "npm run static && npm run sprites && ng build --configuration=staging",
    "build-prod": "npm run static && npm run sprites && ng build --prod --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-pipes": "^10.0.0",
    "bootstrap": "^4.4.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.6.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsdoc": "^3.6.4",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "postcss-cli": "^6.1.3",
    "svg-sprite": "^1.5.0"
  }
}
