"clear-sprites": "rm -r src/resources/img/icons/sprites",
"spriteset-legacy": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/legacy/*.svg --symbol-sprite=legacy.sprite.svg",
"spriteset-arrows": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/arrows/*.svg --symbol-sprite=arrows.sprite.svg",
"spriteset-emotions-colored": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/emotions-colored/*.svg --symbol-sprite=emotions-colored.sprite.svg",
"spriteset-general": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/general/*.svg --symbol-sprite=general.sprite.svg",
"spriteset-icons-colored": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/icons-colored/*.svg --symbol-sprite=icons-colored.sprite.svg",
"spriteset-legend": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/legend/*.svg --symbol-sprite=legend.sprite.svg",
"spriteset-payment": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/payment/*.svg --symbol-sprite=payment.sprite.svg",
"spriteset-social": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/social/*.svg --symbol-sprite=social.sprite.svg",
"spriteset-train-logo": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/train-logo/*.svg --symbol-sprite=train-logo.sprite.svg",
"spriteset-transport": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/transport/*.svg --symbol-sprite=transport.sprite.svg",
"spriteset-transport-colored": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/transport-colored/*.svg --symbol-sprite=transport-colored.sprite.svg",
"spriteset-trip-details": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/trip-details/*.svg --symbol-sprite=trip-details.sprite.svg",
"spriteset-office-menu": "svg-sprite --symbol --dest=src/resources/img/icons/sprites src/resources/img/icons/source/office-menu/*.svg --symbol-sprite=office-menu.sprite.svg",
"spritesheets": "npm run spriteset-legacy & npm run spriteset-arrows & npm run spriteset-emotions-colored & npm run spriteset-general & npm run spriteset-icons-colored & npm run spriteset-legend & npm run spriteset-payment & npm run spriteset-social && npm run spriteset-train-logo & npm run spriteset-transport & npm run spriteset-transport-colored & npm run spriteset-trip-details & npm run spriteset-office-menu",
"sprites": "npm run clear-sprites && npm run spritesheets",
